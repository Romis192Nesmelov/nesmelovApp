{
  "expo": {
    "name": "nesmelovApp",
    "slug": "nesmelovApp",
    "version": "1.0.1",
    "orientation": "default",
    "icon": "./assets/icon_frog.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_frog.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "nesmelov.apk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
